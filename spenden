module.exports = {
	name: 'spenden',
	description: 'Replies with Pong',
	permissions: [],
	execute(message, args, user, bot, event, plugins) {
		bot.sendMessage(`Mein Spendenlink @${user}  https://www.tipeeestream.com/bunnyzudemkushtv/donation Ich bedanke mich für die Unterstützung!❤️`);
	},
};
