console.log('loaded insta')
module.exports = {
	name: 'insta',
	description: 'Replies with Pong',
	permissions: [],
	execute(message, args, user, bot, event, plugins) {
		bot.sendMessage(`Mein Instagram @${user}  https://www.instagram.com/cookie_mein_baby/ `);
	},
};
